{
  "publisher": "ospoon",
  "name": "var-conv-next",
  "displayName": "var-conv-next",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.3.0",
  "description": "Code variable naming quick conversion plug-in suitable for VSCode",
  "author": "OSpoon <zxin088@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ospoon/var-conv-next#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ospoon/var-conv-next"
  },
  "bugs": {
    "url": "https://github.com/ospoon/var-conv-next/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@antfu/ni": "^0.21.12",
    "@types/node": "^20.14.2",
    "@types/vscode": "^1.90.0",
    "@vscode/vsce": "^2.27.0",
    "bumpp": "^9.4.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.5.0",
    "esno": "^4.7.0",
    "pnpm": "^9.3.0",
    "rimraf": "^5.0.7",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "change-case": "^5.4.4"
  }
}
